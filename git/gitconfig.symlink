[core]
	editor = /usr/bin/vim
[user]
	name = Rusty Geldmacher
	email = russell.geldmacher@gmail.com
[color]
 branch = auto
 diff = auto
 status = auto
[include]
 path = ~/.githubrc
[push]
  default = current
[alias]
  track = !sh -c 'git config \"branch.$1.remote\" $0 && git config \"branch.$1.merge\" \"refs/heads/$1\"'
  commit = commit -v
  rmbr = "!f() { git branch -d \"$1\"; git push origin :$1; }; f"
  co = checkout
[hub]
  protocol = https
[core]
	editor = /usr/bin/vim
